			<td>Jenis Kelamin</td>
			<td>
				<input type="radio" />Pria
				<input type="radio" />Wanita 
			</td>
		</tr>
		
		<tr>
			<td>Pekerjaan</td>
			<td>
			<select name="job">
				<option>Pilot</option>
				<option>Polisi</option>
				<option>Tentara</option>
			</select>
			</td>
		</tr>
		
		<tr>
			<td>
				<input type="submit" value="ok" />
				<input type="submit" value="cancel" />
			</td>
		</tr>
		
	</table>
	
	</form>
	
</body>

